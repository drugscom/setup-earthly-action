{
	"problemMatcher": [
		{
			"owner": "golangci-lint",
			"pattern": [
				{
					"regexp": "\\+golangci-lint\\x1b\\[0m\\s\\|\\s(?:/app/)?([^\\s].*):(\\d+):(\\d+):\\s+([^(]+)\\s\\(([^)]+)\\)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"message": 4,
					"code": 5
				}
			]
		},
		{
			"owner": "golangci-lint-warnings",
			"pattern": [
				{
					"regexp": "\\+golangci-lint\\x1b\\[0m\\s\\|\\slevel=(\\w+)\\smsg=\"\\[([^]]+)\\]\\s(.*)\"$",
					"severity": 1,
					"code": 2,
					"message": 3
				}
			]
		}
	]
}
