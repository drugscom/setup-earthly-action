{
	"problemMatcher": [
		{
			"owner": "psalm",
			"pattern": [
				{
					"regexp": "\\+psalm.*failed.*\\s\\|\\s(?:/app/)?([^\\s].*):(\\d+):(\\d+):(\\w+)\\s-\\s(\\w+):\\s(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"code": 5,
					"message": 6
				}
			]
		}
	]
}
