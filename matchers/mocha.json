{
	"problemMatcher": [
		{
			"owner": "mocha",
			"pattern": [
				{
					"regexp": "\\+mocha\\x1b\\[0m\\s\\|\\s\\[\"fail\",.*,\"file\":\"(?:/app/)?(.*)\",\"duration\":.*,\"stack\":\"(.*)\\\\n\\s+at\\sContext[^(]+\\s\\([^:]+:(\\d+):(\\d+)\\).*$",
					"file": 1,
					"message": 2,
					"line": 3,
					"column": 4
				}
			]
		}
	]
}
