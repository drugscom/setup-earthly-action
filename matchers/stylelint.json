{
	"problemMatcher": [
		{
			"owner": "stylelint",
			"pattern": [
				{
					"regexp": "\\+stylelint\\x1b\\[0m\\s\\|\\s(?:/app/)?([^\\s].*):(\\d+):(\\d+):\\s+(.*)\\s\\((.*)\\) \\[(.*)\\]$",
					"file": 1,
					"line": 2,
					"column": 3,
					"message": 4,
					"code": 5,
					"severity": 6
				}
			]
		}
	]
}
