{
	"problemMatcher": [
		{
			"owner": "yamllint",
			"pattern": [
				{
					"regexp": "\\+yamllint.*failed.*\\s\\|\\s(?:/app/|\\./)?([^\\s].*):(\\d+):(\\d+):\\s\\[(\\w+)\\]\\s(.*)\\((.*)\\)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5,
					"code": 6
				}
			]
		}
	]
}
