{
	"problemMatcher": [
		{
			"owner": "phpcs",
			"pattern": [
				{
					"regexp": "\\+phpcs\\x1b\\[0m\\s\\|\\s(?:/app/)?([^\\s].*):(\\d+):(\\d+):\\s(\\w+)\\s-\\s(.*?)(?:\\s\\((.*)\\))?$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5,
					"code": 6
				}
			]
		}
	]
}
