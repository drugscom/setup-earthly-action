{
	"problemMatcher": [
		{
			"owner": "parallel-lint",
			"pattern": [
				{
					"regexp": "\\+parallel-lint.*failed.*\\s\\|\\s+<file name=\"(?:/app/|\\./)?([^\"]+)\">$",
					"file": 1
				},
				{
					"regexp": "\\+parallel-lint.*failed.*\\s\\|\\s+<error\\sline=\"(\\d+)\"\\sseverity=\"(\\w+)\"\\smessage=\"(.*)\"\\ssource=\"(.*)\"\\s?/>$",
					"line": 1,
					"severity": 2,
					"message": 3,
					"code": 4,
					"loop": true
				}
			]
		}
	]
}
